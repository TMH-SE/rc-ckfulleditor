{
  "name": "rc-ckfulleditor",
  "version": "1.0.0",
  "description": "React Ckeditor 5 - easy to integrate",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "npm run build:es && npm run build:webpack && npm run copy",
    "build:es": "rm -rf lib es && tsc && tsc -m es6 --outDir es",
    "build:webpack": "rm -rf _bundles && webpack --mode production",
    "copy": "cp _bundles/ckeditor.js lib/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ckeditor-full",
